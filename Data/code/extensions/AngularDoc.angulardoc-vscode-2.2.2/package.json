{
	"name": "angulardoc-vscode",
	"displayName": "AngularDoc for Visual Studio Code",
	"version": "2.2.2",
	"publisher": "AngularDoc",
	"description": "Architectural analysis and visualization for Angular 2 projects",
	"keywords": [
		"Angular2",
		"Angular",
		"TypeScript",
		"JavaScript"
	],
	"author": {
		"name": "AngularDoc, Inc."
	},
	"homepage": "http://angulardoc.io",
	"bugs": "https://github.com/AngularDoc/angulardoc.github.io/issues",
	"icon": "images/logo128.png",
	"galleryBanner": {
		"color": "#00695C",
		"theme": "light"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"private": true,
	"engines": {
		"vscode": "^1.12.0"
	},
	"categories": [
		"Languages",
		"Other"
	],
	"main": "./build/main",
	"activationEvents": [
		"workspaceContains:package.json",
		"workspaceContains:tsconfig.json",
		"workspaceContains:angular-cli.json",
		"onLanguage:typescript",
		"onCommand:ng.component",
		"onCommand:ng.module",
		"onCommand:ng.class",
		"onCommand:ng.interface",
		"onCommand:ng.enum",
		"onCommand:ng.directive",
		"onCommand:ng.service",
		"onCommand:ng.pipe",
		"onCommand:ngdoc"
	],
	"contributes": {
		"commands": [
			{
				"command": "ng.component",
				"title": "ng g component"
			},
			{
				"command": "ng.module",
				"title": "ng g module"
			},
			{
				"command": "ng.pipe",
				"title": "ng g pipe"
			},
			{
				"command": "ng.class",
				"title": "ng g class"
			},
			{
				"command": "ng.directive",
				"title": "ng g directive"
			},
			{
				"command": "ng.service",
				"title": "ng g service"
			},
			{
				"command": "ng.interface",
				"title": "ng g interface"
			},
			{
				"command": "ng.enum",
				"title": "ng g enum"
			},
			{
				"command": "ngdoc",
				"title": "ngdoc"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "ng.component",
					"group": "Add files@1"
				},
				{
					"command": "ng.directive",
					"group": "Add files@2"
				},
				{
					"command": "ng.pipe",
					"group": "Add files@3"
				},
				{
					"command": "ng.service",
					"group": "Add files@4"
				},
				{
					"command": "ng.class",
					"group": "Add files@5"
				},
				{
					"command": "ng.interface",
					"group": "Add files@6"
				},
				{
					"command": "ng.enum",
					"group": "Add files@7"
				},
				{
					"command": "ng.module",
					"group": "Add files@8"
				}
			]
		},
		"configuration": {
			"id": "angulardoc",
			"type": "object",
			"title": "AngularDoc configuration",
			"properties": {
				"angulardoc.username": {
					"type": "string",
					"default": null,
					"description": "AngularDoc user name."
				},
				"angulardoc.email": {
					"type": "string",
					"default": null,
					"description": "AngularDoc user email."
				},
				"angulardoc.access_token": {
					"type": "string",
					"default": null,
					"description": "AngularDoc user access token."
				},
				"angulardoc.fileFilter": {
					"type": "array",
					"default": [
						"**/!(*.*spec).ts"
					],
					"description": "The file filter of AngularDoc's data collector."
				},
				"angulardoc.directoryFilter": {
					"type": "array",
					"default": [
						"!typings",
						"!node_modules",
						"!.vscode"
					],
					"description": "The directory filter of AngularDoc's data collector."
				}
			}
		}
	},
	"scripts": {
		"build": "tsc && npm run copy && webpack && gulp && npm run copy:main",
		"vscode:prepublish": "tsc -p ./ && npm run copy",
		"copy": "npm run copy:client && npm run copy:bower_components",
		"copy:main": "cp main.js ./build/",
		"copy:client": "cp -r src/browser out/src",
		"copy:build_bower_components": "cp -r bower_components build/",
		"copy:bower_components": "cp -r bower_components out/",
		"compile": "tsc -watch -p ./ && npm run copy",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.32",
		"babel-preset-es2015": "^6.18.0",
		"broccoli-es6modules": "^1.2.3",
		"chalk": "^1.1.3",
		"child_process": "^1.0.2",
		"gulp": "^3.9.1",
		"gulp-babel": "^6.1.2",
		"gulp-concat": "^2.6.0",
		"gulp-imagemin": "^3.1.0",
		"gulp-jshint": "^2.0.2",
		"gulp-minify": "0.0.14",
		"gulp-minify-css": "^1.2.4",
		"gulp-ng-annotate": "^2.0.0",
		"gulp-rename": "^1.2.2",
		"gulp-typescript": "^3.0.2",
		"gulp-uglify": "^2.0.0",
		"jshint": "^2.9.4",
		"json-loader": "^0.5.4",
		"webpack": "^2.1.0-beta.25",
		"webpack-dev-server": "^2.1.0-beta.9",
		"webpack-node-externals": "^1.5.4"
	},
	"dependencies": {
		"@angular/compiler-cli": "^4.0.0",
		"@angular/compiler": "^4.0.0",
		"@angular/core": "^4.0.0",
		"rxjs": "^5.2.0",
		"zone.js": "^0.8.4",
		"@types/node": "^6.0.40",
		"comment-json": "^1.1.3",
		"htmlencode": "0.0.4",
		"jquery": "^3.1.0",
		"moment": "^2.14.1",
		"path": "^0.12.7",
		"readdirp": "^2.1.0",
		"request": "^2.75.0",
		"typescript": "^2.2.1",
		"util": "^0.10.3",
		"uuid": "^2.0.3",
		"vscode": "^1.0.0",
		"vscode-languageclient": "^2.2.1",
		"vscode-languageserver": "^2.2.0",
		"ws": "^2.0.3"
	},
	"__metadata": {
		"id": "656418ba-af03-4c69-bb6e-f3e51d543ca6",
		"publisherId": "0e9fa480-fb5b-4dd8-9bfa-a02e90fa8d8c",
		"publisherDisplayName": "AngularDoc.io"
	}
}